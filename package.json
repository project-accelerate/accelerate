{
  "name": "accelerate-project",
  "version": "0.0.0",
  "engines": {
    "nodejs": "^8.0.0",
    "yarn": "^1.3.2"
  },
  "private": true,
  "scripts": {
    "db:init-dev": "yarn db:revert && yarn db:migrate && yarn db:seed",
    "db:migrate": "knex-migrate up",
    "db:seed": "knex seed:run",
    "db:revert": "knex-migrate down --to 0",
    "db:add-migration": "knex migrate:make",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pretty": "prettier --ignore-path .gitignore --write --tab-width 2 \"**/*.js\"",
    "postinstall": "node scripts/bootstrap",
    "precommit": "lint-staged && yarn test",
    "test": "node scripts/test"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.1",
    "nodemon": "^1.17.1",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-relay": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "date-fns": "^1.29.0",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "fetch": "^1.1.0",
    "geoip-lite": "^1.2.1",
    "knex": "^0.15.2",
    "knex-migrate": "^1.5.1",
    "knex-postgis": "^0.5.0",
    "lodash": "^4.17.5",
    "markov-generator": "^1.2.2",
    "node-fetch": "^2.0.0",
    "pg": "^7.4.1",
    "recompose": "^0.26.0",
    "relay-compiler": "^1.4.1",
    "uuid": "^3.2.1"
  },
  "lint-staged": {
    "**/*.js": [
      "yarn lint:fix",
      "yarn pretty",
      "git add"
    ]
  }
}
