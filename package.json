{
  "name": "accelerate-project",
  "engines": {
    "nodejs": "8"
  },
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pretty": "prettier --ignore-path .gitignore --write --tab-width 2 \"**/*.js\"",
    "postinstall": "gulp setup",
    "heroku-postbuild": "gulp build",
    "db:migrate": "knex-migrate up",
    "db:seed": "knex seed:run",
    "db:revert": "knex-migrate down --to 0",
    "precommit": "lint-staged && npm test"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "husky": "^0.14.3",
    "linklocal": "^2.8.1",
    "lint-staged": "^6.0.1",
    "prettier": "^1.9.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-relay": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "date-fns": "^1.29.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "fetch": "^1.1.0",
    "geoip-lite": "^1.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-shell": "^0.6.5",
    "knex": "^0.14.2",
    "knex-migrate": "^1.5.1",
    "knex-postgis": "^0.5.0",
    "lodash": "^4.17.4",
    "markov-generator": "^1.2.2",
    "node-fetch": "^1.7.3",
    "pg": "^7.4.1",
    "recompose": "^0.26.0",
    "relay-compiler": "^1.4.1",
    "uuid": "^3.2.1"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint:fix",
      "npm run pretty",
      "git add"
    ]
  }
}
